{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "impliedStrict": true,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    },
  },
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "semi": [1, "never"],
  },
  "env": {
    "es6": true,
    "node": true
  }
}
