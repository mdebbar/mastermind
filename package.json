{
  "name": "mastermind",
  "version": "0.0.1",
  "description": "JavaScript implementation of the Mastermind game.",
  "author": "Mouad Debbar <mouad.debbar@gmail.com>",
  "scripts": {
    "console": "babel-node",
    "hot": "node hot-reload-server.js",
    "build:webpack": "webpack --progress --display-error-details",
    "build:dev": "npm run build:webpack -- --config webpack/dev.config.js",
    "build:prod": "NODE_ENV=production npm run build:webpack -- -p --config webpack/prod.config.js",
    "test": "mocha --recursive --compilers js:babel-core/register tests",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.5",
    "express": "^4.13.4",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^2.4.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
